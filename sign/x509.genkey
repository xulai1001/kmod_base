[req]
default_bits = 4096
distinguished_name = dist_name
prompt = no
string_mask = utf8only
x509_extensions = ext

[dist_name]
O = Islab
CN = islab keyserver
emailAddress = admin@islab.org

[ext]
basicConstraints=critical,CA:FALSE
keyUsage=digitalSignature
subjectKeyIdentifier=hash
authorityKeyIdentifier=keyid

